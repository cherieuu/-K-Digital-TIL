   "source": [
    "def factorial(num):\n",
    "    if num<=1:\n",
    "        print('1 반환')\n",
    "        return 1\n",
    "    print(f'{num} * {num-1} 호출!')\n",
    "    retVal=factorial(num-1)\n",
    "    \n",
    "    print(f'{num} * {num-1}! (={retVal}) 호출!')\n",
    "    return num * retVal"
   ]
  },

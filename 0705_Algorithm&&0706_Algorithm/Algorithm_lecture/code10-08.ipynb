   "source": [
    "tab = ''\n",
    "def pow(x, n):\n",
    "    global tab\n",
    "    tab += ' '\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    print(tab + f'{x} * {x}^({n}-{1})')\n",
    "    return x * pow(x, n-1)"
   ]
